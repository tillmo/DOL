+#################################################################
+#
+#    Annotation properties
+#
+#################################################################
+
+
+skos:prefLabel rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+dol:propagatesToAdjoint rdf:type owl:AnnotationProperty ;
+                        
+                        rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege.
+Domain: owl:Class
+Range: xsd:boolean"""@en ,
+                                     "The subject class denotes a kind of mapping that, if it holds for a translation, also holds for the adjoint reduction."@en .
+
+
+
+dol:reifies rdf:type owl:AnnotationProperty ;
+            
+            rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege
+Domain: owl:Class
+Range: owl:ObjectProperty"""@en ,
+                         "The subject class reifies the object property."@en .
+
+
+
+dol:strictlyStrongerThan rdf:type owl:AnnotationProperty .
+
+
+
+skos:editorialNote rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+skos:example rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+dol:reifiedBy rdf:type owl:AnnotationProperty ;
+              
+              rdfs:comment """Developers' note: This is just for informative purposes: It doesn't work for annotation properties
+InverseOf: reifies"""@en ,
+                           "The subject property is reified by the object class."@en .
+
+
+
+skos:scopeNote rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+skos:note rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+dol:supportsSymbolKind rdf:type owl:AnnotationProperty ;
+                       
+                       rdfs:comment """Developers' note: This is just for informative purposes: Even though it probably does not go beyond OWL, it doesn't work for annotation properties in Protege
+Domain: OMSLanguage
+Range: owl:Class that rdfs:subClassOf value NonLogicalSymbol"""@en .
+
+
+
+skos:definition rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+dol:symbolKindIn rdf:type owl:AnnotationProperty ;
+                 
+                 rdfs:comment """Developers' note: This is just for informative purposes: It doesn't work for annotation properties
+InverseOf: supportsSymbolKind"""@en .
+
+
+
+skos:altLabel rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .
+
+
+
+
+
+#################################################################
+#
+#    Datatypes
+#
+#################################################################
+
+
+
+
+#################################################################
+#
+#    Object Properties
+#
+#################################################################
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/conservativelyExtendsTo
+
+dol:conservativelyExtendsTo rdf:type owl:ObjectProperty ;
+                            
+                            rdfs:label "conservatively extends to"@en ;
+                            
+                            rdfs:comment "The object is a conservative extension of the subject"@en ;
+                            
+                            rdfs:domain dol:ConservativeExtensionMapping ;
+                            
+                            rdfs:subPropertyOf dol:extendsTo ,
+                                               dol:isLogicallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/definitionallyExtendsTo
+
+dol:definitionallyExtendsTo rdf:type owl:ObjectProperty ;
+                            
+                            rdfs:label "definitionally extends to"@en ;
+                            
+                            rdfs:comment "the object is a definitional extension of the subject"@en ;
+                            
+                            rdfs:domain dol:DefinitionalExtensionMapping ;
+                            
+                            rdfs:subPropertyOf dol:conservativelyExtendsTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/entails
+
+dol:entails rdf:type owl:ObjectProperty ;
+            
+            rdfs:label "entails"@en ;
+            
+            skos:definition "relation of logical entailment, expressing that all models of the subject are also models of the object"@en ;
+            
+            rdfs:subPropertyOf dol:isLogicallyRelatedTo ;
+            
+            rdfs:domain [ rdf:type owl:Class ;
+                          owl:unionOf ( dol:OMS
+                                        dol:OMSNetwork
+                                      )
+                        ] ;
+            
+            rdfs:range [ rdf:type owl:Class ;
+                         owl:unionOf ( dol:OMS
+                                       dol:OMSNetwork
+                                       dol:Sentence
+                                     )
+                       ] .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/extendsTo
+
+dol:extendsTo rdf:type owl:ObjectProperty ;
+              
+              rdfs:label "extends to"@en ;
+              
+              rdfs:comment "The subject is a mapping of an extension, the object is the target of the extension mapping, i.e. the extended OMS."@en ;
+              
+              rdfs:domain dol:ExtensionMapping ;
+              
+              rdfs:range dol:OMS ;
+              
+              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ,
+                                 dol:mapsTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/formalizes
+
+dol:formalizes rdf:type owl:ObjectProperty ;
+               
+               rdfs:label "formalizes"@en ;
+               
+               rdfs:comment "The subject is a precise mathematical entity capturing the object (which is an informal or semi-formal entity)"@en ;
+               
+               owl:inverseOf dol:isFormalizedBy ;
+               
+               rdfs:subPropertyOf dol:isSemanticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasAlignment
+
+dol:hasAlignment rdf:type owl:ObjectProperty ;
+                 
+                 rdfs:label "has alignment"@en ;
+                 
+                 rdfs:comment "The subject is a DOL library, in which the object is an alignment between two participating OMS."@en ;
+                 
+                 rdfs:range dol:Alignment ;
+                 
+                 rdfs:subPropertyOf dol:hasMapping ;
+                 
+                 owl:inverseOf dol:isAlignmentIn ;
+                 
+                 rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasExactLogicalExpressivity
+
+dol:hasExactLogicalExpressivity rdf:type owl:ObjectProperty ;
+                                
+                                rdfs:label "has exact logical expressivity"@en ;
+                                
+                                rdfs:comment "The subject language has exactly the expressivity of the object logic."@en ;
+                                
+                                rdfs:subPropertyOf dol:isSemanticallyRelatedTo ,
+                                                   dol:supportsLogic .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasHeterogeneousEnvironment
+
+dol:hasHeterogeneousEnvironment rdf:type owl:ObjectProperty ;
+                                
+                                rdfs:label "has heterogeneous environment"@en ;
+                                
+                                skos:definition "relationship between the subject and a heterogeneous environment in which it is embedded or is associated with."@en ;
+                                
+                                rdfs:range dol:HeterogeneousEnvironment ;
+                                
+                                rdfs:subPropertyOf dol:isMathematicallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasInterpretation
+
+dol:hasInterpretation rdf:type owl:ObjectProperty ;
+                      
+                      rdfs:label "has interpretation"@en ;
+                      
+                      rdfs:comment "The subject is a DOL library, in which the object is an interpretation between two participating OMS."@en ;
+                      
+                      rdfs:range dol:Interpretation ;
+                      
+                      rdfs:subPropertyOf dol:hasMapping ;
+                      
+                      owl:inverseOf dol:isInterpretationIn ;
+                      
+                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasLanguageAspect
+
+dol:hasLanguageAspect rdf:type owl:ObjectProperty ;
+                      
+                      rdfs:label "has language aspect"@en ;
+                      
+                      skos:definition "the relation between an OMS language and its language aspects"@en ;
+                      
+                      rdfs:range dol:LanguageAspect ;
+                      
+                      rdfs:domain dol:OMSLanguage ;
+                      
+                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasLogic
+
+dol:hasLogic rdf:type owl:FunctionalProperty ,
+                      owl:ObjectProperty ;
+             
+             rdfs:label "has logic"@en ;
+             
+             rdfs:comment """Developers' note: The reasoners in Protege don't like us to say the following, which makes hasLogic a non-simple property.  hasLogic is not used in cardinality restrictions, so it _should_ be OK to say the following, but in practice it doesn't work.
+SubPropertyChain: semanticallyDenotes o hasLogic"""@en ;
+             
+             skos:definition "specification of valid reasoning that comprises signatures (user defined vocabularies), models (interpretations of these), sentences (constraints on models), and a satisfaction relation between models and sentences."@en ;
+             
+             rdfs:comment "the logic in which an OMS is formalized"@en ;
+             
+             rdfs:range dol:Logic ;
+             
+             rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
+             
+             rdfs:domain [ rdf:type owl:Class ;
+                           owl:unionOf ( dol:OMS
+                                         dol:OMSSemanticObject
+                                       )
+                         ] .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasMapping
+
+dol:hasMapping rdf:type owl:ObjectProperty ;
+               
+               rdfs:label "has mapping"@en ;
+               
+               rdfs:comment "The subject is a DOL library, in which the object is a mapping between two participating OMS."@en ;
+               
+               rdfs:range dol:OMSMapping ;
+               
+               rdfs:subPropertyOf dol:hasMember ,
+                                  dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasMember
+
+dol:hasMember rdf:type owl:ObjectProperty ;
+              
+              rdfs:label "has member"@en ;
+              
+              skos:definition "The inverse of the set membership relation."@en ;
+              
+              rdfs:range dol:MathematicalEntity ;
+              
+              rdfs:domain dol:Set ;
+              
+              rdfs:subPropertyOf dol:isMathematicallyRelatedTo ;
+              
+              owl:inverseOf dol:isMemberOf .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasModels
+
+dol:hasModels rdf:type owl:ObjectProperty ;
+              
+              rdfs:label "has models"@en ;
+              
+              skos:definition "an OMS (resp. its semantic object) has an assoicated class of models, which is the class of models of the OMS"@en ;
+              
+              rdfs:range dol:ClassOfModels ;
+              
+              rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
+              
+              rdfs:domain [ rdf:type owl:Class ;
+                            owl:unionOf ( dol:OMS
+                                          dol:OMSSemanticObject
+                                        )
+                          ] .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasOMS
+
+dol:hasOMS rdf:type owl:ObjectProperty ;
+           
+           rdfs:label "has OMS"@en ;
+           
+           rdfs:comment "The subject is a DOL library, in which the object is a participating heterogeneous OMS."@en ;
+           
+           rdfs:range dol:OMS ;
+           
+           rdfs:subPropertyOf dol:hasMember ,
+                              dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasPart
+
+dol:hasPart rdf:type owl:ObjectProperty ;
+            
+            rdfs:label "has part"@en ;
+            
+            skos:definition "relationship between two things were the second is a component of the first"@en .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasProficiency
+
+dol:hasProficiency rdf:type owl:ObjectProperty ;
+                   
+                   rdfs:label "has proficiency"@en ;
+                   
+                   skos:definition "property of a tool that is capable of implementing a process."@en ;
+                   
+                   rdfs:range dol:Process ;
+                   
+                   rdfs:domain dol:Tool .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasSentences
+
+dol:hasSentences rdf:type owl:ObjectProperty ;
+                 
+                 rdfs:label "has sentences"@en ;
+                 
+                 skos:definition "an OMS (resp. its semantic object) has an assoicated set of sentences, which is the set of sentences of the OMS"@en ;
+                 
+                 rdfs:range dol:SetOfSentences ;
+                 
+                 rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
+                 
+                 rdfs:domain [ rdf:type owl:Class ;
+                               owl:unionOf ( dol:OMS
+                                             dol:OMSSemanticObject
+                                           )
+                             ] .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasSequenceMember
+
+dol:hasSequenceMember rdf:type owl:ObjectProperty ;
+                      
+                      rdfs:label "has sequence member"@en ;
+                      
+                      skos:definition "The inverse of the sequence membership relation."@en ;
+                      
+                      skos:note "this is a lingustic property, because it is about sequences of linguistic entities that occur in a text."@en ;
+                      
+                      rdfs:subPropertyOf dol:isLinguisticallyRelatedTo ;
+                      
+                      owl:inverseOf dol:isMemberOf .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasSignature
+
+dol:hasSignature rdf:type owl:ObjectProperty ;
+                 
+                 rdfs:label "has signature"@en ;
+                 
+                 skos:definition "an OMS (resp. its semantic object) has an assoicated signature, which is the signature of the OMS"@en ;
+                 
+                 rdfs:range dol:Signature ;
+                 
+                 rdfs:subPropertyOf dol:isSemanticallyRelatedTo ;
+                 
+                 rdfs:domain [ rdf:type owl:Class ;
+                               owl:unionOf ( dol:OMS
+                                             dol:OMSSemanticObject
+                                           )
+                             ] .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasSource
+
+dol:hasSource rdf:type owl:FunctionalProperty ,
+                       owl:ObjectProperty ;
+              
+              rdfs:label "has source"@en ;
+              
+              skos:definition "the source OMS of a mapping"@en ;
+              
+              rdfs:range dol:OMS ;
+              
+              rdfs:domain dol:OMSMapping ;
+              
+              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/hasTarget
+
+dol:hasTarget rdf:type owl:FunctionalProperty ,
+                       owl:ObjectProperty ;
+              
+              rdfs:label "has target"@en ;
+              
+              skos:definition "the target OMS of a mapping"@en ;
+              
+              rdfs:range dol:OMS ;
+              
+              rdfs:domain dol:OMSMapping ;
+              
+              rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/impliedExtendsTo
+
+dol:impliedExtendsTo rdf:type owl:ObjectProperty ;
+                     
+                     rdfs:label "implied extends to"@en ;
+                     
+                     skos:definition "the relation of a model-theoretic conservative extension that does not introduce new non-logical symbols"@en ;
+                     
+                     rdfs:domain dol:ImpliedExtensionMapping ;
+                     
+                     rdfs:subPropertyOf dol:definitionallyExtendsTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isAdjointOf
+
+dol:isAdjointOf rdf:type owl:ObjectProperty ;
+                
+                rdfs:label "is adjoint of"@en ;
+                
+                rdfs:comment "The subject is a translation (or reduction) that shares its sentence and model translation with the object reduction (or translation). The signature translations are adjoint to each other (in the sense of category theory)."@en ;
+                
+                rdfs:range dol:LogicReduction ;
+                
+                rdfs:domain dol:LogicTranslation ;
+                
+                rdfs:subPropertyOf dol:isMathematicallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isAlignmentIn
+
+dol:isAlignmentIn rdf:type owl:ObjectProperty ;
+                  
+                  rdfs:label "is alignment in"@en ;
+                  
+                  rdfs:comment "The obbject is a DOL library, in which the subject is an alignment between two participating OMS."@en ;
+                  
+                  rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isApproximantOf
+
+dol:isApproximantOf rdf:type owl:ObjectProperty ,
+                             owl:TransitiveProperty ;
+                    
+                    rdfs:label "is approximant of"@en ;
+                    
+                    skos:definition "approximation (in the sense of a logically implied theory, possibly aftersuitable translation) of an OMS in a smaller signature or OMS language"@en ;
+                    
+                    rdfs:domain dol:OMS ;
+                    
+                    rdfs:range dol:OMS ;
+                    
+                    rdfs:subPropertyOf dol:isEntailedBy .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isCombinationOf
+
+dol:isCombinationOf rdf:type owl:ObjectProperty ;
+                    
+                    rdfs:label "is combination of OMS"@en ;
+                    
+                    skos:example "Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being."@en ;
+                    
+                    skos:definition "aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network "@en ;
+                    
+                    rdfs:domain dol:Combination ;
+                    
+                    rdfs:range dol:OMSNetwork ;
+                    
+                    rdfs:subPropertyOf dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isCompositionOf
+
+dol:isCompositionOf rdf:type owl:ObjectProperty ;
+                    
+                    rdfs:label "is composition of"@en ;
+                    
+                    rdfs:comment "Developers' note: The subject mapping is the composition of the object list of mappings (list item 1  list item 2    list item n).  All elements of the latter list must be instances of the same class as the subject.  (Enforcing the latter constraint goes beyond OWL.)"@en ,
+                                 "Range can't be specified in OWL DL"@en ;
+                    
+                    rdfs:subPropertyOf dol:isMathematicallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isConsequenceTheoreticallyConservativeExtensionOf
+
+dol:isConsequenceTheoreticallyConservativeExtensionOf rdf:type owl:ObjectProperty ;
+                                                      
+                                                      rdfs:label "is consequence theoretically conservative extension of"@en ;
+                                                      
+                                                      rdfs:comment "The subject OMS is a consequence-theoretic conservative extension of the object OMS. A consequence-theoretic conservative extension is an extension that does not add new theorems (in terms of the unextended signature)."@en ;
+                                                      
+                                                      rdfs:subPropertyOf dol:isConservativeExtensionOf .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isConservativeExtensionOf
+
+dol:isConservativeExtensionOf rdf:type owl:ObjectProperty ;
+                              
+                              rdfs:label "is conservative extension of"@en ;
+                              
+                              rdfs:comment "the subject is a conservative extension of the object"@en ;
+                              
+                              rdfs:subPropertyOf dol:isExtensionOf ,
+                                                 dol:isLogicallyRelatedTo ;
+                              
+                              owl:inverseOf dol:isModuleOf ;
+                              
+                              owl:propertyChainAxiom ( dol:isMappedBy
+                                                       dol:conservativelyExtendsTo
+                                                     ) .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isDefinitionalExtensionOf
+
+dol:isDefinitionalExtensionOf rdf:type owl:ObjectProperty ;
+                              
+                              rdfs:label "is definitional extension of"@en ;
+                              
+                              skos:note " $O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$. "@en ,
+                                        " An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$."@en ,
+                                        " Each definitional extension is also a monomorphic extension but not vice versa. "@en ;
+                              
+                              rdfs:comment "Developers' note: We need FOL, or the same reification approach as chosen for the different kinds of translations, for capturing that an extension is definitional if and only if it is weakly definitional and model-theoretically conservative."@en ;
+                              
+                              skos:definition "extension whose newly introduced non-logical symbols are interpreted in a unique way"@en ;
+                              
+                              rdfs:subPropertyOf dol:isModelTheoreticallyConservativeExtensionOf ;
+                              
+                              dol:strictlyStrongerThan dol:isMonomorphicExtensionOf ;
+                              
+                              rdfs:subPropertyOf dol:isMonomorphicExtensionOf ,
+                                                 dol:isWeakDefinitionalExtensionOf ;
+                              
+                              owl:propertyChainAxiom ( dol:isMappedBy
+                                                       dol:definitionallyExtendsTo
+                                                     ) .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isElementOf
+
+dol:isElementOf rdf:type owl:ObjectProperty ;
+                
+                rdfs:label "is element of OMS"@en ;
+                
+                skos:definition "any resource in an OMS (e.g. a non-logical symbol, a sentence, a correspondence, the OMS itself, ...) or a named set of such resources."@en ;
+                
+                rdfs:range dol:OMS ;
+                
+                rdfs:subPropertyOf dol:hasPart ,
+                                   dol:isLinguisticallyRelatedTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isEmbeddableInto
+
+dol:isEmbeddableInto rdf:type owl:ObjectProperty ,
+                              owl:TransitiveProperty ;
+                     
+                     rdfs:label "is embeddable into"@en ;
+                     
+                     rdfs:comment "The subject is embeddable into the object logic."@en ;
+                     
+                     rdfs:subPropertyOf dol:isExactlyMappableTo ,
+                                        dol:isMappableToBijectivelyOnModels ,
+                                        dol:isMappableToLogic ,
+                                        dol:isTranslatableTo .
+
+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/isEntailedBy
+
+dol:isEntailedBy rdf:type owl:ObjectProperty ;
+                 
+                 rdfs:label "is entailed by"@en ;
+                 
+                 skos:definition "inverse of relation of logical entailment"@en ;
+                 
+                 owl:inverseOf dol:entails ;
+                 
