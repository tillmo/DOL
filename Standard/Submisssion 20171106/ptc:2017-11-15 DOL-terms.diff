+        <rdfs:range>
+            <owl:Class>
+                <owl:unionOf rdf:parseType="Collection">
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/OMSNetwork"/>
