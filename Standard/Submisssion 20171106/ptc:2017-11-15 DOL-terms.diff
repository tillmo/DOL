+###  http://www.omg.org/spec/DOL/DOL-terms/SimpleTheoroidalMapping
+
+dol:SimpleTheoroidalMapping rdf:type owl:Class ;
+                            
+                            rdfs:label "simple theoroidal mapping"@en ;
+                            
+                            owl:equivalentClass [ rdf:type owl:Class ;
+                                                  owl:intersectionOf ( dol:LogicMapping
+                                                                       [ rdf:type owl:Class ;
+                                                                         owl:complementOf dol:PlainMapping
+                                                                       ]
+                                                                     )
+                                                ] ;
+                            
+                            rdfs:subClassOf dol:LogicMapping ;
+                            
+                            dol:propagatesToAdjoint "true"^^xsd:boolean ;
+                            
+                            skos:definition "logic mapping that maps signatures to theories"@en ;
+                            
+                            rdfs:comment "orthogonal to WeaklyExactMapping and FaithfulMapping"@en ,
+                                         "the resulting theory tpyically contains infrastructure axioms that are needed for expressing the logic mapping"@en ;
+                            
+                            dol:reifies dol:isSimplyTheoroidallyMappableTo .
+[ rdf:type owl:Axiom ;
+  rdfs:comment "Developers' note: Protg 4.2 doesn't like 'Class: OMSMapping DisjointUnionOf: PlainMapping, SimpleTheoroidalMapping', so we do it this way:"@en ;
