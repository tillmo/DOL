+                                         
+                                         skos:note "Developers' note: When scaling this ontology implementation up to DOL, look below for annotations marked as 'beyond OWL'.  These things can be modeled in FOL."^^xsd:string ;
+                                         
+                                         sm:fileAbbreviation "dol"^^xsd:string ;
+                                         
+                                         sm:contentLanguage "http://www.omg.org/spec/ODM/"^^xsd:anyURI ;
+                                         
