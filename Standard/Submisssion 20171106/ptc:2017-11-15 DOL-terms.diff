+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="&dol;hasSentences"/>
+                <owl:someValuesFrom rdf:resource="&dol;SetOfSentences"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="&dol;hasLogic"/>
+                <owl:someValuesFrom rdf:resource="&dol;Logic"/>
