+
+
+###  http://www.omg.org/spec/DOL/DOL-terms/SatisfiableOMS
+
+dol:SatisfiableOMS rdf:type owl:Class ;
+                   
+                   rdfs:label "satisfiable OMS"@en ;
+                   
+                   owl:equivalentClass [ rdf:type owl:Class ;
+                                         owl:intersectionOf ( dol:OMS
+                                                              [ rdf:type owl:Class ;
+                                                                owl:complementOf dol:UnsatisfiableOMS
+                                                              ]
+                                                            )
+                                       ] ;
+                   
+                   rdfs:subClassOf dol:ConsistentOMS ;
