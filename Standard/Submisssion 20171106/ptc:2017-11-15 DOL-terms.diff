+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <rdfs:label xml:lang="en">approximant</rdfs:label>
+        <skos:definition xml:lang="en">approximation (in the sense of a logically implied theory, possibly after suitable translation)  of an OMS in a smaller signature or a sublanguage.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Approximation -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Approximation">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">approximation</rdfs:label>
+        <skos:definition xml:lang="en">structured OMS that expresses a maximum approximant.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/AxiomSentence -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/AxiomSentence">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Sentence"/>
+        <rdfs:label xml:lang="en">axiom sentence</rdfs:label>
+        <skos:definition xml:lang="en">sentence postulated to be valid (i.e.\ true in every model)</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/BasicOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/BasicOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/FlattenableOMS"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/NativeOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/semanticallyDenotes"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicalTheory"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMSDocumentation"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:comment xml:lang="en">a basic OMS (in a single language, logic, and serialization)</rdfs:comment>
+        <rdfs:label xml:lang="en">basic OMS</rdfs:label>
+        <skos:definition xml:lang="en">unstructured native OMS that may be used as a building block for a larger OMS, denoting a logical theory</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Category -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Category">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:label xml:lang="en">category</rdfs:label>
+        <skos:definition xml:lang="en">a class of objects with suitable morphisms between them</skos:definition>
+        <skos:note xml:lang="en">In DOL, objects of a category are usually signatures or OMS, and morphisms are signature morphisms, or OMS mappings. In principle, no assumption about the exact nature of objects and morphisms is made.</skos:note>
+        <skos:note xml:lang="en">The morphisms determine which part of the structure of the objects is relevant, i.e. preserved by morphisms. Hence, objects can be seen as &apos;sets with structure&apos;, and morphisms as &apos;structure-preserving maps&apos;.  However note that not all categories can be obtained in this way.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ClassOfModels -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ClassOfModels">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SemanticEntity"/>
+        <rdfs:label xml:lang="en">class of models</rdfs:label>
+        <skos:definition xml:lang="en">a class of models (over some signature in some logic)</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Closure -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Closure">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">closure</rdfs:label>
+        <skos:definition xml:lang="en">structured OMS expressing a variant of the closed world assumption by restricting the models to those that are minimal or maximal (with respect to the local environment). The closed world assumption is a default assumption about facts whose status in unknown.</skos:definition>
+        <skos:note xml:lang="en">DOL supports four different forms of closure: minimization and maximization as well as freeness and cofreeness.</skos:note>
+        <skos:note xml:lang="en">Symbols from the local environment are assumed to have a fixed interpretation. Only the symbols newly declared in the closure are forced to have minimal or maximal interpretation.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Cofreeness -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Cofreeness">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Closure"/>
+        <rdfs:label xml:lang="en">cofreeness</rdfs:label>
+        <skos:definition xml:lang="en">special type of closure, restriction of models to those that are cofree (with respect to the local environment).</skos:definition>
+        <skos:note xml:lang="en">In first-order-like logics, cofreeness means maximal interpretation of predicates and equality being observable equivalence. Cofreeness can be used for the specification of coinductive datatypes like infinite lists and streams.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/CollectionOfExpressions -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/CollectionOfExpressions">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:label xml:lang="en">collection of expressions</rdfs:label>
+        <skos:definition xml:lang="en">collection whose members are expressions</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Combination -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Combination">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMSNetwork"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">combination</rdfs:label>
+        <skos:definition xml:lang="en">structured OMS expressing the aggregation of all the OMS in an OMS network, where non-logical symbols are shared according to the OMS mappings in the OMS network </skos:definition>
+        <skos:example xml:lang="en">Consider an ontology involving a concept Person, and another one involving Human being, and an alignment that relates these to concepts. In the combination of the ontologies along the alignment, there is only one concept, representing both Person and Human being.</skos:example>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtension">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtension"/>
+        <rdfs:label xml:lang="en">consequence-theoretic conservative extension</rdfs:label>
+        <skos:definition xml:lang="en">extension that does not add new theorems (in terms of the unextended signature).</skos:definition>
+        <skos:note xml:lang="en">An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ConsequenceTheoreticConservativeExtensionMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtensionMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isConsequenceTheoreticallyConservativeExtensionOf"/>
+        <rdfs:label xml:lang="en">consequence-theoretic conservative extension mapping</rdfs:label>
+        <skos:definition xml:lang="en">extension that does not add new theorems (in terms of the unextended signature)</skos:definition>
+        <skos:note xml:lang="en">An extension $O_2$ of an OMS $O_1$ is a consequence-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtension">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Extension"/>
+        <rdfs:label xml:lang="en">conservative extension</rdfs:label>
+        <skos:definition xml:lang="en">extension that does not add new logical properties with respect to the signature of the extended OMS.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtensionMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ExtensionMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isConservativeExtensionOf"/>
+        <rdfs:label xml:lang="en">conservative extension mapping</rdfs:label>
+        <skos:definition xml:lang="en">extension mapping of a consequence-theoretic or model-theoretic conservative extension</skos:definition>
+        <skos:note xml:lang="en">If used without qualification, the consequence-theoretic version is meant.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ConsistentOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ConsistentOMS">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+                    <owl:Class>
+                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/InconsistentOMS"/>
+                    </owl:Class>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <rdfs:label xml:lang="en">consistent OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS that has a non-trivial set of logical consequences in the sense that not every sentence follows from the OMS.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Correspondence -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Correspondence">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLEntity"/>
+        <rdfs:label xml:lang="en">correspondence</rdfs:label>
+        <skos:definition xml:lang="en">relationship between an non-logical symbol $e_1$ from an OMS $O_1$ and an non-logical symbol $e_2$ from an OMS $O_2$, or between an non-logical symbol $e_1$ from $O_1$ and a term $t_2$ formed from non-logical symbols\index{non-logical symbol from $O_2$.</skos:definition>
+        <skos:note xml:lang="en">A correspondence is given as a quadruple 
+$(e_1, R, e_2, c)$,
+where $R$ denotes the type of relationship that is asserted to hold between the two non-logical 
+symbols/terms, and 0&lt;=c&lt;=1 is a confidence value.  $R$ and $c$ may be omitted: When $R$ is 
+omitted, it defaults to the equivalence relation, unless another default
+relation has been explicitly specified; when $c$ is omitted, it defaults to 1.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DOLDocument -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DOLDocument">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLEntity"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Document"/>
+        <rdfs:label xml:lang="en">DOL document</rdfs:label>
+        <skos:definition xml:lang="en">document containing a DOL library.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DOLEntity -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DOLEntity">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:label xml:lang="en">DOL entity</rdfs:label>
+        <skos:definition xml:lang="en">entity that is used in DOL texts.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DOLLibrary -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DOLLibrary">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/semanticallyDenotes"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/GlobalEnvironment"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">DOL library</rdfs:label>
+        <skos:definition xml:lang="en">collection of named OMS and OMS networks, possibly written in different OMS languages, linked by named OMS mappings.</skos:definition>
+        <skos:note xml:lang="en">collection is used here in the sense of syntactic collection</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/StructuredOMS"/>
+        <rdfs:label xml:lang="en">DOL structured OMS</rdfs:label>
+        <skos:definition xml:lang="en">syntactically valid DOL expression denoting an OMS that is built from smaller OMS as building blocks.</skos:definition>
+        <skos:note xml:lang="en">DOL structured OMS, typically, use basic OMS as building blocks for defining other structured OMS, OMS mappings or OMS networks.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DefaultMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DefaultMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping"/>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <rdfs:label xml:lang="en">default mapping</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that will be chosen by default if only the source and target logic are given</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtension">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtension"/>
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/WeakDefinitionalExtension"/>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MonomorphicExtension"/>
+        <rdfs:label xml:lang="en">definitional extension</rdfs:label>
+        <skos:definition xml:lang="en">extension whose newly introduced non-logical symbols are interpreted in a unique way.</skos:definition>
+        <skos:note xml:lang="en">An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtensionMapping">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtensionMapping"/>
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/WeakDefinitionalExtensionMapping"/>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MonomorphicExtensionMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isDefinitionalExtensionOf"/>
+        <rdfs:label xml:lang="en">definitional extension mapping</rdfs:label>
+        <skos:definition xml:lang="en">extension whose newly introduced non-logical symbols are interpreted in a unique way</skos:definition>
+        <skos:note xml:lang="en">$O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$.</skos:note>
+        <skos:note xml:lang="en">An extension $O_2$ of an OMS $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.</skos:note>
+        <skos:note xml:lang="en">Each definitional extension is also a monomorphic extension but not vice versa.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Document -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Document">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:label xml:lang="en">document</rdfs:label>
+        <skos:definition xml:lang="en">result of serializing an OMS or DOL library using a given serialization.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ElusiveOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ElusiveOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/FlattenableOMS"/>
+        <rdfs:label xml:lang="en">elusive OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS that is not flattenable</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Embedding -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Embedding">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ExactMapping"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicTranslation"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ModelBijectiveMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isEmbeddableInto"/>
+        <rdfs:label xml:lang="en">embedding</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that embeds the source into the target logic, using components that are embeddings and (in the case of model translations) isomorphism</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Entailment -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Entailment">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMSMapping"/>
+        <rdfs:label xml:lang="en">entailment</rdfs:label>
+        <skos:altLabel xml:lang="en">logical consequence</skos:altLabel>
+        <skos:definition xml:lang="en">relation between two OMS (or an OMS and a sentence) expressing that the second one is logically implied by the first one.</skos:definition>
+        <skos:note xml:lang="en">Entailment expresses that each model satisfying the first OMS also satisfies the second OMS (or the sentence, respectively).</skos:note>
+        <skos:note xml:lang="en">Entailments can also be stated between networks, or networks and OMS.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Equivalence -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Equivalence">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMSMapping"/>
+        <rdfs:label xml:lang="en">equivalence</rdfs:label>
+        <skos:definition xml:lang="en">OMS mapping ensuring that two OMS share the same definable concepts</skos:definition>
+        <skos:note xml:lang="en"> Two OMS are equivalent if they have a common definitional extension. The OMS may be written in different OMS languages. </skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ExactMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ExactMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/WeaklyExactMapping"/>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isExactlyMappableTo"/>
+        <rdfs:label xml:lang="en">exact mapping</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that is compatible with certain DOL structuring constructs</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Expression -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Expression">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/usesLanguage"/>
+                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Language"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">expression</rdfs:label>
+        <skos:definition xml:lang="en">a finite combination of symbols that are well-formed according to applicable rules (depending on the language)</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Extension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Extension">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">extension</rdfs:label>
+        <skos:definition xml:lang="en">structured OMS extending a given OMS with new symbols and sentences.</skos:definition>
+        <skos:note xml:lang="en">The new symbols and sentences are interpreted relative to the
+local envorinment, which is the signature of the &apos;given OMS&apos;.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ExtensionMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMSMapping"/>
+        <rdfs:label xml:lang="en">extension mapping</rdfs:label>
+        <skos:definition xml:lang="en">mapping between two OMS where the sets of non-logical symbols and sentences of the second OMS are supersets of those present in the first OMS</skos:definition>
+        <skos:editorialNote xml:lang="en">This definition differs from that in the Terms and Definition section of the DOL specification by focussing on the mapping rather than the codomain of the mapping.</skos:editorialNote>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/FaithfulMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/FaithfulMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping"/>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isFaithfullyMappableTo"/>
+        <rdfs:label xml:lang="en">faithful mapping</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that preserves and reflects logical consequence</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Filtering -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Filtering">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">filtering</rdfs:label>
+        <skos:definition xml:lang="en">structured OMS expressing the verbatim removal of symbols or axioms from an OMS.</skos:definition>
+        <skos:note xml:lang="en">If a symbol is removed, all axioms containing that symbol are removed, too.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/FlattenableOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/FlattenableOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <rdfs:label xml:lang="en">flattenable OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS that can be seen, by purely syntactical means, to be logically equivalent to a basic OMS</skos:definition>
+        <skos:note xml:lang="en">More precisely, an OMS is flattenable if and only if it is either a basic OMS or it is an extension, union, translation, module extraction, approximation, filtering, or reference of named OMS involving only flattenable OMS.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Flattening -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Flattening">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Process"/>
+        <rdfs:label xml:lang="en">flattening</rdfs:label>
+        <skos:definition xml:lang="en">process of transforming an OMS to an equivalent basic OMS</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/FormalSemantics -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/FormalSemantics">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SemanticEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/formalizes"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Language"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SemanticDomain"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SemanticRule"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">formal semantics</rdfs:label>
+        <skos:definition xml:lang="en">assignment of a mathematical meaning to a language by mapping abstract syntax to suitable semantic domains</skos:definition>
+        <skos:note xml:lang="en">A formal semantics is a formalization of the meaning of a language.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Freeness -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Freeness">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Closure"/>
+        <rdfs:label xml:lang="en">freeness</rdfs:label>
+        <skos:definition xml:lang="en">special type of closure, restriction of models to those that are free (with respect to the local environment).</skos:definition>
+        <skos:note xml:lang="en">In first-order logic (and similar logics), freeness means minimal interpretation of predicates and minimal equality among data values. Freeness can be used for the specification of inductive datatypes like numbers, lists, trees, bags etc. In order to specify e.g. lists over some elements, the specification of the elements should be in the local environment.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/GlobalEnvironment -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/GlobalEnvironment">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LocalEnvironment"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">global environment</rdfs:label>
+        <skos:definition xml:lang="en">mapping from identifiers (IRIs) to values in  semantics domains representing the global knowledge about OMS.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Graph -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Graph">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:label xml:lang="en">graph</rdfs:label>
+        <skos:altLabel xml:lang="en">network</skos:altLabel>
+        <skos:definition xml:lang="en">set of objects (nodes) that are connected by links (edges)</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousEnvironment -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousEnvironment">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LanguageGraph"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicGraph"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SupportsRelation"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">heterogeneous environment</rdfs:label>
+        <skos:definition xml:lang="en">environment for the expression of homogeneous and heterogeneous OMS, comprising a logic graph, an OMS language graph and a supports relation</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/HeterogeneousOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <owl:disjointWith rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/HomogeneousOMS"/>
+        <rdfs:label xml:lang="en">heterogeneous OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS whose parts are supported by different logics</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/HomogeneousOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/HomogeneousOMS">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <rdfs:label xml:lang="en">homogeneous OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS whose parts are supported by one logic</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtension">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtension"/>
+        <rdfs:label xml:lang="en">implied extension</rdfs:label>
+        <skos:definition xml:lang="en">model-theoretic conservative extension that does not introduce new non-logical symbols.</skos:definition>
+        <skos:note xml:lang="en">A conservative extension $O_2$ of an OMS
+    $O_1$ is an implied extension, if and only if the signature of
+    $O_2$ is the signature of $O_1$.  $O_2$ is an implied extension of
+    $O_1$ if and only if the model class of $O_2$ is the model class
+    of $O_1$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ImpliedExtensionMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DefinitionalExtensionMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isImpliedExtensionOf"/>
+        <rdfs:label xml:lang="en">implied extension mapping</rdfs:label>
+        <skos:definition xml:lang="en">model-theoretic conservative extension that does not introduce new non-logical symbols</skos:definition>
+        <skos:note xml:lang="en">A conservative extension $O_2$ of an OMS $O_1$ is an implied extension, if and only if the signature of $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of $O_1$ if and only if the model class of $O_2$ is the model class of $O_1$.</skos:note>
+        <skos:note xml:lang="en">Each implied extension is also a definitional extension but not vice versa.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Import -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Import">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/DOLStructuredOMS"/>
+        <rdfs:label xml:lang="en">import</rdfs:label>
+        <skos:definition xml:lang="en">reference to an OMS behaving as if it were verbatim included; also import of DOL libraries.</skos:definition>
+        <skos:example xml:lang="en">An owl:import in OWL is an import.</skos:example>
+        <skos:note xml:lang="en">Importing $O_1$ into $O_2$ turns $O_2$ into an extension of $O_1$.</skos:note>
+        <skos:note xml:lang="en">Semantically, an import of $O_2$ into $O_1$ is equivalent to the verbatim inclusion of $O_2$ in place of the import declaration.</skos:note>
+        <skos:note xml:lang="en">The import of a whole DOL library into another DOL library is also called import.</skos:note>
+        <skos:note xml:lang="en">The purpose of $O_2$ importing $O_1$ is to make non-logical symbols and sentences of $O_1$ available in $O_2$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/InconsistentOMS -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/InconsistentOMS">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <rdf:Description rdf:about="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+                    <owl:Class>
+                        <owl:complementOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ConsistentOMS"/>
+                    </owl:Class>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/UnsatisfiableOMS"/>
+        <rdfs:label xml:lang="en">inconsistent OMS</rdfs:label>
+        <skos:definition xml:lang="en">OMS that has a trivial set of logical consequences in the sense that every sentence follows from the OMS.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Institution -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Institution">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/formalizes"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Logic"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Category"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Model"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SatisfactionRelation"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Sentence"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Signature"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SignatureMorphism"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">institution</rdfs:label>
+        <skos:definition xml:lang="en">metaframework mathematically formalising the notion of a logic</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/InterfaceSignature -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/InterfaceSignature">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Signature"/>
+        <rdfs:label xml:lang="en">interface signature</rdfs:label>
+        <skos:definition xml:lang="en">signature mediating between an OMS and a module of that OMS in the sense that it contains those non-logical symbols that the sentences of the module and the sentences of the OMS have in common.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Interpretation -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Interpretation">
+        <owl:equivalentClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/View"/>
+        <rdfs:label xml:lang="en">interpretation</rdfs:label>
+        <skos:altLabel xml:lang="en">refinement</skos:altLabel>
+        <skos:altLabel xml:lang="en">view</skos:altLabel>
+        <skos:definition xml:lang="en">OMS mapping that postulates a specialization relation between two OMS along a morphism between their signatures</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Language -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Language">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/CollectionOfExpressions"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">language</rdfs:label>
+        <skos:definition xml:lang="en">collection of expressions</skos:definition>
+        <skos:note xml:lang="en">collection is used here in the sense of syntactic collection</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LanguageAspect -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LanguageAspect">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity"/>
+        <rdfs:label xml:lang="en">language aspect</rdfs:label>
+        <skos:definition xml:lang="en">set of language constructs of a given language, not necessarily forming a sublanguage</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LanguageGraph -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LanguageGraph">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Graph"/>
+        <rdfs:label xml:lang="en">OMS language graph</rdfs:label>
+        <skos:definition xml:lang="en">graph of OMS languages and OMS language translations, typically used in a heterogeneous environment.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LanguageMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LanguageMapping">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsFrom"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Language"/>
+                    </owl:Restriction>
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsTo"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Language"/>
+                    </owl:Restriction>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Mapping"/>
+        <rdfs:label xml:lang="en">language mapping</rdfs:label>
+        <skos:definition xml:lang="en">mapping between languages</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LinguisticEntity">
+        <rdfs:label xml:lang="en">linguistic entity</rdfs:label>
+        <skos:definition xml:lang="en">entity that is of linguistic, syntactic nature.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LinkedData -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LinkedData">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/WebEntity"/>
+        <rdfs:label xml:lang="en">linked data</rdfs:label>
+        <skos:definition xml:lang="en">structured data that is published on the Web in a machine-processable way, according to principles explained in http://linkeddata.org/</skos:definition>
+        <skos:note xml:lang="en"> RDF, serialized as RDF/XML \cite{W3C:REC-rdf-syntax-grammar-20040210}, is the most common format for publishing linked data. However, its usage is not mandatory. </skos:note>
+        <skos:note xml:lang="en"> The linked data principles (adapted from \cite{BernersLee:LinkedData2006} and its paraphrase at \cite{Wikipedia:LinkedData2011}) are the following: \begin{enumerate} \item Use IRIs as names for things. \item Use HTTP IRIs so that these things can be referred to and looked up (``dereferenced&apos;&apos;) by people and user agents.\footnote{I.e., the IRI is treated as a URL (uniform resource locator).} \item Provide useful machine-processable (plus optionally human-readable) information about the thing when its IRI is dereferenced, using standard formats. \item Include links to other, related IRIs in the exposed data to improve discovery of other related information on the Web. \end{enumerate} </skos:note>
+        <skos:note xml:lang="en"> Using HTTP content negotiation \cite{rfc2616} it is possible to serve representations in different formats from the same URL. </skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LocalEnvironment -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LocalEnvironment">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Signature"/>
+        <rdfs:label xml:lang="en">local environment</rdfs:label>
+        <skos:definition xml:lang="en">signature built from all previously-declared symbols and axioms.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Logic -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Logic">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isFormalizedBy"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Institution"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:comment xml:lang="en">a logic that defines the semantics of an language</rdfs:comment>
+        <rdfs:label xml:lang="en">logic</rdfs:label>
+        <skos:definition xml:lang="en">specification of valid reasoning that comprises signatures, sentences, models, and a satisfaction relation between models and sentences</skos:definition>
+        <skos:example xml:lang="en"> $\mathcal{SROIQ}(D)$ is the logic underlying OWL 2 DL. </skos:example>
+        <skos:note xml:lang="en"> Most OMS languages have an underlying logic. </skos:note>
+        <skos:note xml:lang="en"> See annex~\ref{a:rdf-logic-vocab} for the organization of the relation between OMS languages and their logics and serializations. </skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicGraph -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicGraph">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Graph"/>
+        <rdfs:label xml:lang="en">logic graph</rdfs:label>
+        <skos:definition xml:lang="en">graph of logics, logic translations and logic reductions, typically used in a heterogeneous environment.</skos:definition>
+        <skos:note xml:lang="en">In a logic graph, some of the logic translations and reductions can be marked to be default translations.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsFrom"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Logic"/>
+                    </owl:Restriction>
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsTo"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Logic"/>
+                    </owl:Restriction>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Mapping"/>
+        <rdfs:subClassOf>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsFrom"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Logic"/>
+                    </owl:Restriction>
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsTo"/>
+                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                        <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Logic"/>
+                    </owl:Restriction>
+                </owl:intersectionOf>
+            </owl:Class>
+        </rdfs:subClassOf>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <rdfs:comment xml:lang="en">a mapping (translation or reduction) between two logics</rdfs:comment>
+        <rdfs:label xml:lang="en">logic mapping</rdfs:label>
+        <skos:definition xml:lang="en">mapping between logics</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicReduction -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicReduction">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping"/>
+        <rdfs:label xml:lang="en">logic reduction</rdfs:label>
+        <skos:definition xml:lang="en">reduction of a source logic onto a (usually less expressive) target logic (mapping signatures, sentences and models) that simply forgets those parts of the logical structure not fitting the target logic</skos:definition>
+        <skos:note xml:lang="en">mapping between logics forgetting parts of the structure, projection to a smaller logic, in contrast to reduction</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicTranslation -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicTranslation">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LogicMapping"/>
+        <rdfs:label xml:lang="en">logic translation</rdfs:label>
+        <skos:definition xml:lang="en">translation of a source logic into a target logic (mapping signatures, sentences and models) that keeps or encodes the logical content of OMS</skos:definition>
+        <skos:note xml:lang="en">mapping between logics representing all structure, in contrast to reduction</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicalLanguageAspect -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicalLanguageAspect">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/LanguageAspect"/>
+        <rdfs:label xml:lang="en">logical language aspect</rdfs:label>
+        <skos:definition xml:lang="en">the (unique) language aspect of an OMS language that enables the expression of non-logical symbols and sentences in a logical language</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/LogicalTheory -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/LogicalTheory">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/SetOfSentences"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasPart"/>
+                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
+                <owl:onClass rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Signature"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">logical theory</rdfs:label>
+        <skos:definition xml:lang="en">signature equipped with a set of sentences over the signature</skos:definition>
+        <skos:note xml:lang="en">The sentences must use only those non-logical symbols that are present in the signature.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/MDEModel -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/MDEModel">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/OMS"/>
+        <rdfs:label xml:lang="en">MDE model</rdfs:label>
+        <skos:definition xml:lang="en">logical theory that is used as a model in the sense of model-driven engineering (MDE)</skos:definition>
+        <skos:note xml:lang="en">Not to be confused with the term \termref{model} in the sense
+of logic (model theory).</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Mapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Mapping">
+        <owl:equivalentClass>
+            <owl:Class>
+                <owl:intersectionOf rdf:parseType="Collection">
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsFrom"/>
+                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
+                    </owl:Restriction>
+                    <owl:Restriction>
+                        <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/mapsTo"/>
+                        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
+                    </owl:Restriction>
+                </owl:intersectionOf>
+            </owl:Class>
+        </owl:equivalentClass>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isMappableTo"/>
+        <rdfs:comment xml:lang="en">a generic mapping (translation or reduction)</rdfs:comment>
+        <rdfs:label xml:lang="en">mapping</rdfs:label>
+        <skos:definition xml:lang="en">relation between a set of inputs and a set of permissible outputs with the property that each input is related to exactly one output.</skos:definition>
+        <skos:note xml:lang="en">alternative term: function</skos:note>
+        <skos:note xml:lang="en">in some cases is a morphism, as in Category Theory.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Matcher -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Matcher">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Tool"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasProficiency"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Matching"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">matcher</rdfs:label>
+        <skos:definition xml:lang="en">tool that implements matching</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Matching -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Matching">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Process"/>
+        <rdfs:label xml:lang="en">matching</rdfs:label>
+        <skos:definition xml:lang="en">algorithmic procedure that generates an alignment for two given OMS.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity">
+        <rdfs:label xml:lang="en">mathematical entity</rdfs:label>
+        <skos:definition xml:lang="en">entity that is part of a mathematical theory.</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Maximization -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Maximization">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Closure"/>
+        <rdfs:label xml:lang="en">maximization</rdfs:label>
+        <skos:definition xml:lang="en">form of closure that restricts the models to those that are maximal (with respect to the local environment).</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/MaximumApproximant -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/MaximumApproximant">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Approximant"/>
+        <rdfs:label xml:lang="en">maximum approximant</rdfs:label>
+        <skos:definition xml:lang="en">best possible (in the sense of a maximum set of logical consequences approximant of an OMS in a smaller signature or a sublanguage.</skos:definition>
+        <skos:note xml:lang="en">Technically, a maximum approximant is a uniform interpolant</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Minimization -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Minimization">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Closure"/>
+        <rdfs:label xml:lang="en">minimization</rdfs:label>
+        <skos:definition xml:lang="en">form of closure that restricts the models to those that are minimal (with respect to the local environment).</skos:definition>
+        <skos:note xml:lang="en">Also known as circumscription.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/Model -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/Model">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/MathematicalEntity"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isModelOfSignature"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Signature"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">model</rdfs:label>
+        <skos:definition xml:lang="en"> semantic interpretation of all non-logical symbols of a signature</skos:definition>
+        <skos:note xml:lang="en">A model of an OMS is a model of the signature of the OMS that also satisfies all the additional constraints expressed by the OMS. In case of flattenable OMS, these constraints are expressed by the axioms of the OMS.</skos:note>
+        <skos:scopeNote xml:lang="en">This term is not to be confused with model (here called MDEModel) in the sense of modeling (i.e., the ``M&apos;&apos; in OMS).</skos:scopeNote>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelBijectiveMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelBijectiveMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ModelExpansiveMapping"/>
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/WeaklyExactMapping"/>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isMappableToBijectivelyOnModels"/>
+        <rdfs:label xml:lang="en">model bijective mapping</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that has a bijective mapping of models</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelExpansiveMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelExpansiveMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/FaithfulMapping"/>
+        <propagatesToAdjoint rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</propagatesToAdjoint>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isModelExpansivelyMappableTo"/>
+        <rdfs:label xml:lang="en">model expansive mapping</rdfs:label>
+        <skos:definition xml:lang="en">logic mapping that has a surjective model translation (ensuring faithfulness of the mapping)</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelFinder -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelFinder">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Tool"/>
+        <rdfs:subClassOf>
+            <owl:Restriction>
+                <owl:onProperty rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/hasProficiency"/>
+                <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ModelFinding"/>
+            </owl:Restriction>
+        </rdfs:subClassOf>
+        <rdfs:label xml:lang="en">model finder</rdfs:label>
+        <skos:definition xml:lang="en">Tool that implements model finding</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelFinding -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelFinding">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/Process"/>
+        <rdfs:label xml:lang="en">model finding</rdfs:label>
+        <skos:definition xml:lang="en">process that finds models of an OMS and thus proves it to be satisfiable</skos:definition>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtension -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtension">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtension"/>
+        <rdfs:label xml:lang="en">model-theoretic conservative extension</rdfs:label>
+        <skos:definition xml:lang="en">extension that does not lead to a restriction of class of models of an OMS.</skos:definition>
+        <skos:note xml:lang="en">An extension $O_2$ of an OMS $O_1$ is a model-theoretic conservative extension, if all properties formulated in the signature of $O_1$ hold for $O_1$ whenever they hold for $O_2$.</skos:note>
+    </owl:Class>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtensionMapping -->
+
+    <owl:Class rdf:about="http://www.omg.org/spec/DOL/DOL-terms/ModelTheoreticConservativeExtensionMapping">
+        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/ConservativeExtensionMapping"/>
+        <reifies rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isModelTheoreticallyConservativeExtensionOf"/>
+        <rdfs:label xml:lang="en">model-theoretic conservative extension mapping</rdfs:label>
