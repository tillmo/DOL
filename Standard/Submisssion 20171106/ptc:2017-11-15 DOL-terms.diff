+    
+    <owl:Class rdf:about="&dol;Minimization">
+        <rdfs:label xml:lang="en">minimization</rdfs:label>
+        <rdfs:subClassOf rdf:resource="&dol;Closure"/>
+        <skos:note xml:lang="en">Also known as circumscription.</skos:note>
+        <skos:definition xml:lang="en">form of closure that restricts the models to those that are minimal (with respect to the local environment).</skos:definition>
+    </owl:Class>
+    
+    <owl:Class rdf:about="&dol;Model">
+        <rdfs:label xml:lang="en">model</rdfs:label>
+        <rdfs:subClassOf rdf:resource="&dol;MathematicalEntity"/>
