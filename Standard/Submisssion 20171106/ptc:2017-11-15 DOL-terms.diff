+        <rdfs:label xml:lang="en">extends to</rdfs:label>
+    </owl:ObjectProperty>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/formalizes -->
+
+    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/DOL-terms/formalizes">
