+                                                       ]
+                                                       [ rdf:type owl:Restriction ;
+                                                         owl:onProperty dol:supportsLogic ;
+                                                         owl:allValuesFrom dol:Logic
+                                                       ]
+                                                       [ rdf:type owl:Restriction ;
+                                                         owl:onProperty dol:supportsSerialization ;
+                                                         owl:allValuesFrom dol:Serialization
+                                                       ]
+                                                     )
+                                ] ,
