+        <rdfs:label xml:lang="en">has alignment</rdfs:label>
+    </owl:ObjectProperty>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/hasExactLogicalExpressivity -->
+
+    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/DOL/DOL-terms/hasExactLogicalExpressivity">
