+        <rdfs:comment xml:lang="en">The object is a conservative extension of the subject</rdfs:comment>
+        <rdfs:label xml:lang="en">conservatively extends to</rdfs:label>
+    </owl:ObjectProperty>
+    
+
+
+    <!-- http://www.omg.org/spec/DOL/DOL-terms/definitionallyExtendsTo -->
