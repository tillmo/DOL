+        <owl:inverseOf rdf:resource="http://www.omg.org/spec/DOL/DOL-terms/isFormalizedBy"/>
+        <rdfs:comment xml:lang="en">The subject is a precise mathematical entity capturing the object (which is an informal or semi-formal entity)</rdfs:comment>
+        <rdfs:label xml:lang="en">formalizes</rdfs:label>
+    </owl:ObjectProperty>
+    
+
+
